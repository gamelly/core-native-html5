{
    "name": "@gamely/core-native-html5",
    "version": "0.0.20",
    "author": "RodrigoDornelles",
    "license": "Apache-2.0",
    "homepage": "https://gamelly.github.io/core-native-html5",
    "repository": "https://github.com/gamelly/core-native-html5",
    "funding": "https://github.com/sponsors/RodrigoDornelles",
    "bugs": "https://github.com/gamelly/core-native-html5/issues",
    "description": "create your own game-engine with just javascript.",
    "browser": "dist/index.js",
    "module": "dist/index.mjs",
    "main": "dist/index.cjs.js",
    "exports": {
        ".": {
            "require": "./dist/index.cjs.js",
            "import": "./dist/index.mjs"
        }
    },
    "keywords": [
        "game engine",
        "game",
        "engine"
    ],
    "scripts": {
        "build:game": "bun build --external=module --outdir=dist --entrypoint index.html --chunk-naming '[dir]/[name].[ext]'",
        "build:html": "bun build lib/browser.js --outfile dist/index.js",
        "build:cjs": "bun build lib/common.js --format cjs --outfile dist/index.cjs.js",
        "build:mjs": "bun build src/index.ts --format esm --outfile dist/index.mjs",
        "build": "bun run build:mjs && bun run build:cjs && bun run build:html",
        "test": "bun test"
    },
    "devDependencies": {
        "wasmoon": "^1.16.0",
        "bun": "^1.2.4"
    }
}
